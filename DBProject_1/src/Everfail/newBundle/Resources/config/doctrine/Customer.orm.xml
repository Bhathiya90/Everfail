<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Everfail\newBundle\Entity\Customer" table="customer">
    <id name="id" type="string" column="ID" length="10">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="custName" type="string" column="Cust_Name" length="45" nullable="false"/>
    <field name="email" type="string" column="Email" length="30" nullable="true"/>
  </entity>
</doctrine-mapping>
